{
    "Log Function": {
        "prefix": [ "$ ^Quillscript.Tools.Log" ],
        "body": [ "$ ^Quillscript.Tools.Log '${1:Hello}' ${2:null} ${3|Log,Success,Warning,Error|}" ],
        "description": [
            "Display a log message.",
            "   (Message: string)            message to display",
            "   (Owner: ref = null)          reference to the owner of the message",
            "   (PrintType: PrintType = Log) [ Log, Success, Warning, Error ]",
            "",
            "Example:",
            "$ ^Quillscript.Tools.Log 'Hello, World!'",
            "$ ^Quillscript.Tools.Log Oops! {&ScriptRef} Error",
            "\n"
        ]
    },

    "Print Function": {
        "prefix": [ "$ ^Quillscript.Tools.Print" ],
        "body": [ "$ ^Quillscript.Tools.Print '${1:Hello}'" ],
        "description": [
            "Display a message",
            "   (Message: string)   message to display",
            "   (Owner: ref = null) reference to the owner of the message",
            "",
            "Example:",
            "$ ^Quillscript.Tools.Print Hello",
            "$ ^Quillscript.Tools.Print 'Hello, World!' {&ScriptRef}",
            "\n"
        ]
    },

    "Success Function": {
        "prefix": [ "$ ^Quillscript.Tools.Success" ],
