    },



    "Tag Once": {
        "prefix": [ "#once" ],
        "body": [ "#once" ],
        "description": "Special tag to make this statement play just once.\n\n"
    },

    "Tag Mark": {
        "prefix": [ "#mark" ],
        "body": [ "#mark" ],
        "description": "Special tag to create a variable for this statement on played.\n\n"
    },



